    "def func_2(dataframe):             \n",
    "    census_df[\"year\"] = np.nan\n",
    "    census_df[\"measurement\"] = np.nan\n",
    "    for row in range(len(census_df)):\n",
    "        thing = census_df.loc[row, \"info\"]\n",
    "        measure = thing[0:7]\n",
    "        if int(thing[7:9])>=97:\n",
    "            yr = \"19\" + str(thing[7:9])\n",
    "        else:\n",
    "            yr= '20'+ str(thing[7:9])\n",
    "        census_df.loc[row,\"year\"] = int(yr)\n",
    "        census_df.loc[row,\"measurement\"] = str(measure)\n",
    "    return(census_df)"
   ]
